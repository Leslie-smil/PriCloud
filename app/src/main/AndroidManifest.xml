<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.scujcc.pricloud">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />


    <application
        android:requestLegacyExternalStorage="true"
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="edu.scujcc.pricloud.Activities.SpManagerActivity" />
        <activity android:name="edu.scujcc.pricloud.Activities.UserInfoActivity" />
        <activity android:name="edu.scujcc.pricloud.Activities.RecycleActivity" />
        <activity android:name="edu.scujcc.pricloud.Activities.SetingActivity" />
        <activity android:name="edu.scujcc.pricloud.Activities.OverLoadActivity" />
        <activity android:name="edu.scujcc.pricloud.Activities.LoadActivity" />
        <activity android:name="edu.scujcc.pricloud.Adapter.contentAdapter" />
        <activity android:name="edu.scujcc.pricloud.Activities.RegisterActivity" />
        <activity android:name="edu.scujcc.pricloud.Activities.PersonalActivity" />
        <activity android:name="edu.scujcc.pricloud.Activities.UpLoadActivity" />
        <activity android:name="edu.scujcc.pricloud.Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="edu.scujcc.pricloud.Activities.MainActivity" />

    </application>

</manifest>